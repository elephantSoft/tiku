.custom_nav {
  width: 100%;
  background: #70DA96;
  position: relative;
  z-index: 99999;
}

.custom_nav_box {
  position: fixed;
  width: 100%;
  background: #70DA96;
  z-index: 99999;
  border-bottom: 1rpx solid rgba(255, 255, 255, 0.3);
}

.custom_nav_bar {
  position: relative;
  z-index: 9;
}
 
.custom_nav_box .nav_title {
  font-size: 28rpx;
  color: black;
  text-align: center;
  position: absolute;
  max-width: 360rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  z-index: 1;
}

.icon-back {
  display: inline-block;
  width: 74rpx;
  padding-left: 16rpx;
  vertical-align: top;
  margin-top: 20rpx;
  height: 100%;
}

.custom_nav_box .back-pre, .custom_nav_box .back-home {
  width: 35rpx;
  height: 35rpx;
  vertical-align: middle;
}
